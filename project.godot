; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="First Proyect"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
Global="*res://Data/Global/GLOBAL.gd"

[dialogic]

directories/dch_directory={
"MainPlayer": "res://Dialogic/Personajes/MainPlayer.dch",
"character": "res://UI/UI DI/character.dch"
}
directories/dtl_directory={
"timeline": "res://Dialogic/Timelines/timeline.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=["res://Dialogic/Styles/Estilo Propio.tres", "res://Dialogic/Styles/Demás.tres", "res://Dialogic/Styles/MainWithPortraits.tres"]
layout/default_style="res://Dialogic/Styles/Demás.tres"
variables={
"MOVERSE": true
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.04
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.3
animations/join_default_wait=true
animations/leave_default_length=0.0
animations/leave_default_wait=true
animations/cross_fade_default_length=1.0
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
timer/process_in_physics=true
portraits/default_portrait=""
animations/join_default="Fade In"
animations/leave_default="Instant Out"

[display]

window/size/viewport_width=650
window/size/viewport_height=340
window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/AS2P/plugin.cfg", "res://addons/dialogic/plugin.cfg", "res://addons/net.yarvis.pixel_pen/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "red",
"res://Componentes/": "orange",
"res://Data/": "purple",
"res://UI/": "green"
}

